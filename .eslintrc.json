{
  "extends": ["next", "next/core-web-vitals"],
  "plugins": [
    "unused-imports",
    "eslint-plugin-import"
  ],
  "rules": {
    "unused-imports/no-unused-imports": "error",
    "no-duplicate-imports": "error",
    "no-unused-vars": [
      "error", {
        "vars": "all",
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
